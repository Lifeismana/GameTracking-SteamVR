// hlvr_start_soundevent
"hlvr_start_soundevent"
{
	"public"
	{
		"operator"		"util_null"
		"operator_variables"
		{
			"sequence_type"
			{
				"data_type"		"enum"
				"enum_field"		"opvar_get_string.array_selection_type"
				"value"		"random_exclusive"
			}
			"soundevents"
			{
				"data_type"		"string"
				"soundevent_data"		"true"
				"value"
				{
					"value0"		"nullsoundevent"
				}
			}
			"restart_on_finish"
			{
				"data_type"		"float"
				"value"		"0.000000"
			}
			"volume_atten"
			{
				"data_type"		"float"
				"value"		"1.000000"
				"_system_properties"
				{
					"display_group"		"locked"
				}
			}
			"attachment"
			{
				"data_type"		"string_token"
				"_system_properties"
				{
					"display_group"		"locked"
				}
			}
			"position"
			{
				"data_type"		"float3"
				"value"		"[0.0, 0.0, 0.0]"
				"_system_properties"
				{
					"display_group"		"locked"
					"display_edit_type"		"field_position_editor"
				}
			}
		}
	}
	"get_event_info"
	{
		"operator"		"soundevent_get_info"
	}
	"should_add_dependent"
	{
		"operator"		"math_float"
		"apply"		"min"
		"input1"		"@public.restart_on_finish"
		"input2"		"@get_event_info.output_unstopped"
	}
	"add_dependent"
	{
		"operator"		"soundevent_control"
		"input_add_dependent"		"@should_add_dependent.output"
	}
	"get_soundevent"
	{
		"execute_once"		"true"
		"operator"		"opvar_get_string"
		"check_event_data"		"true"
		"input_operator_name"		"public"
		"input_field_name"		"soundevents"
		"input_use_namespace"		"false"
		"array_selection_type"		"@public.sequence_type"
	}
	"start_soundevent"
	{
		"operator"		"soundevent_start"
		"execute_once"		"false"
		"input_spawn_oneshots"		"0.000000"
		"input_execute"		"1"
		"input_start"		"@get_event_info.output_unstopped"
		"soundevent_name"		"@get_soundevent.output"
		"input_retrigger_on_finish"		"@public.restart_on_finish"
		"pass_opvar_args"		"true"
		"input_update_opvar_args"		"true"
		"operator_variables"
		{
			"public.volume_atten"
			{
				"operator_arg_field"		"true"
				"data_type"		"float"
				"value"		"@public.volume_atten"
			}
			"public.attachment"
			{
				"operator_arg_field"		"true"
				"data_type"		"string_token"
				"value"		"@public.attachment"
			}
			"public.position"
			{
				"operator_arg_field"		"true"
				"data_type"		"float3"
				"value"		"@public.position"
			}
		}
	}
}

