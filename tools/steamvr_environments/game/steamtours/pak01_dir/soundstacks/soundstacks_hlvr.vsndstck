	{
		"operator"		"math_float"
		"apply"		"div"
		"input1"		"1"
		"input2"		"@slice_length.output"
	}
	"mult_mod_time"
	{
		"operator"		"math_float"
		"apply"		"mult"
		"input1"		"@mod_time.output"
		"input2"		"@slice_ratio.output"
	}
	"get_animated_position"
	{
		"operator"		"math_animate_float3"
		"input_execute"		"1"
		"input_animation_pct"		"@mult_mod_time.output"
		"input_float3_start"		"@public.position_start"
		"input_float3_end"		"@public.position_end"
		"input_distance_scalar"		"1"
		"input_animation_direction"		"0"
		"anim_type"		"lerp"
	}
	"delta1"
	{
		"operator"		"math_delta"
		"input"		"@mod_time.output"
	}
	"test_delta"
	{
		"input_execute"		"1.000000"
		"operator"		"math_float"
		"apply"		"greater_than"
		"input1"		"@delta1.output"
		"input2"		"0.000000"
	}
	"delta_and_unstopped"
	{
		"input_execute"		"1.000000"
		"operator"		"math_float"
		"apply"		"min"
		"input1"		"@test_delta.output"
		"input2"		"@get_event_info.output_unstopped"
	}
	"stop_at_pct"
	{
		"input_execute"		"1.000000"
		"operator"		"math_float"
		"apply"		"less_than"
		"input1"		"@mult_mod_time.output"
		"input2"		"@public.stop_at_pct"
	}
	"stop_at_pct_and_unstopped"
	{
		"input_execute"		"1.000000"
		"operator"		"math_float"
		"apply"		"min"
		"input1"		"@stop_at_pct.output"
		"input2"		"@get_event_info.output_unstopped"
	}
	"should_add_dependent"
	{
		"operator"		"math_float"
		"apply"		"min"
		"input1"		"@public.restart_on_finish"
		"input2"		"@get_event_info.output_unstopped"
	}
	"add_dependent"
	{
		"operator"		"soundevent_control"
		"input_add_dependent"		"@get_event_info.output_unstopped"
	}
	"get_soundevent"
	{
		"execute_once"		"true"
		"operator"		"opvar_get_string"
		"check_event_data"		"true"
		"input_operator_name"		"public"
		"input_field_name"		"soundevents"
		"input_use_namespace"		"false"
		"array_selection_type"		"@public.sequence_type"
	}
	"offset_fade_over_time"
	{
		"input_execute"		"1.000000"
		"operator"		"math_float"
		"apply"		"add"
		"input1"		"@public.offset_fade_over_time"
		"input2"		"@get_event_info.output_plat_time"
	}
	"remap_fade_over_time"
	{
		"operator"		"math_curve_2d"
		"input"		"@offset_fade_over_time.output"
		"public_field_name"		"public.volume_fade_over_time_curve"
		"input_loop_start"		"@public.volume_fade_over_time_loop_start"
		"input_loop_end"		"@public.volume_fade_over_time_loop_end"
	}
	"start_soundevent"
	{
		"operator"		"soundevent_start"
		"execute_once"		"false"
		"input_spawn_oneshots"		"0.000000"
		"input_execute"		"1"
		"input_start"		"@test_delta.output"
		"soundevent_name"		"@get_soundevent.output"
		"input_retrigger_on_finish"		"@public.restart_on_finish"
		"pass_opvar_args"		"true"
		"input_update_opvar_args"		"true"
		"operator_variables"
		{
			"public.volume_atten"
			{
				"operator_arg_field"		"true"
				"data_type"		"float"
				"value"		"@remap_fade_over_time.output"
			}
			"public.attachment"
			{
				"operator_arg_field"		"true"
				"data_type"		"string_token"
				"value"		"@public.attachment"
			}
		}
	}
	"soundevent_set_position"
	{
		"operator"		"soundevent_param"
		"input_guid"		"@start_soundevent.output_current_guid"
		"input_execute"		"@public.send_position"
		"operator_variables"
		{
			"public.position"
			{
				"operator_arg_field"		"true"
				"data_type"		"float3"
				"value"		"@get_animated_position.output"
			}
		}
	}
}

